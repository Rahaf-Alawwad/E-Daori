<h1>Favorite Team</h1> 

<!-- begin -->

  <% for (let index = response.length-1; index >= 0; index--) {%> 

    <% const element = response[index];%> 


  <div class="card"  onclick=" location.href = '/testing/match/details?matchID=<%= element.fixture.matchID %>' " data-tilt data-tilt-max="50" data-tilt-speed="400" data-tilt-perspective="500">
 
    <div class="content">
      <div class="Row-img"> 

        <img  src="<%= element.teams.home.logo %>" alt="Card image cap">



        <img  src="<%= element.teams.away.logo %>" alt="Card image cap">
        
       

      </div>
      <div class="Row-name-under-img">
        <div class="card-body"> 
          <div class="card-title"> <a href="/team/details?teamID=<%= element.teams.home.id %>"><%= element.teams.home.name %></a><a href="/team/details?teamID=<%= element.teams.away.id %>"><%= element.teams.away.name %></a></div>
        </div>

  
      </div>
      <p class="card-text"><%= element.score.fulltime.home %> &nbsp - &nbsp <%= element.score.fulltime.away %></p>
          
            <h4  >Time:<%= element.fixture.timestamp %>  </h4 >
            <h4  >Time:<%= element.fixture.date %>  </h4 >
            <h4  >Referee:<%= element.fixture.referee %>  </h4 >
            <h4  ><%= element.fixture.venue.city %> <%= element.fixture.venue.name %></h4 >
         
            <div class="card-body">
            <a href="#" class="card-link">Another link</a>
          </div>
            
    </div>
        </div>
  <% }%>


  <!-- end -->
