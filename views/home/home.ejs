
<!-- begin -->


  <% for (let index = response.response.length-1; index >= 0; index--) {%> 

    <% const element = response.response[index];%> 

    


<div class="card" style="width: 18rem;">
    <div class="row">
    
        <div class="column">
        <img class="card-img-top" src="<%= element.teams.home.logo %>" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title"><%= element.teams.home.name %></h5>
        </div>
    
    </div>

    <div class="column">
        <img class="card-img-top" src="<%= element.teams.away.logo %>" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title"><%= element.teams.away.name %></h5>
        </div>
    </div>
   
</div>
<p class="card-text"><%= element.score.fulltime.home %> &nbsp - &nbsp <%= element.score.fulltime.away %></p>
    <ul class="list-group list-group-flush">
      <li class="list-group-item">Time:<%= element.fixture.timestamp %>  </li>
      <li class="list-group-item">Time:<%= element.fixture.date %>  </li>
      <li class="list-group-item">Referee:<%= element.fixture.referee %>  </li>
      <li class="list-group-item"><%= element.fixture.venue.city %> <%= element.fixture.venue.name %></li>
    </ul>
    <div class="card-body">
      <a href="#" class="card-link">Another link</a>
    </div>
    
    <% let played= element.score.fulltime.home %>
    <script>if( played == null){console.log("button deactivated"); document.getElementById("voteBtn").setAttribute('disabled', true)}</script> 
    <a href="/vote" ><button id= "voteBtn" type="submit">Vote</button></a>

    
  </div>
  <% }%>
  <!-- end -->